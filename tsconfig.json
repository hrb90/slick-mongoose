{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "module": "commonjs",
    "lib": ["dom","es2016","dom.iterable","scripthost"],
    "target": "es5",
    "jsx": "react",
    "allowJs": true
  }
}
